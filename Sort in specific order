class Solution {
    
    
    void sort(int[] arr){
        for(int i=0;i < arr.length;i++){
            if(arr[i]%2 !=0){
                arr[i] = -arr[i];
            }
        }        
    }
    public void sortIt(int[] arr) {
        // code here
        

        sort(arr);
        Arrays.sort(arr);
        sort(arr);
    }
}
